ifndef CONFIG
  CONFIG=Debug
endif

ifeq ($(CONFIG),Debug)
  BINDIR := ../../..
  LIBDIR := ../../..
  OBJDIR := obj/Debug
  OUTDIR := ../../..
  CPPFLAGS := -MD -D "HAVE_MMAP" -I "../../"
  CFLAGS += $(CPPFLAGS) -fPIC -g
  CXXFLAGS := $(CFLAGS)
  LDFLAGS += -L$(BINDIR) -L$(LIBDIR) -shared -dynamiclib
  LDDEPS :=
  TARGET := libclaro-base.so
  BLDCMD = $(CC) -o $(OUTDIR)/$(TARGET) $(OBJECTS) $(LDFLAGS) $(RESOURCES)
endif

ifeq ($(CONFIG),Release)
  BINDIR := ../../..
  LIBDIR := ../../..
  OBJDIR := obj/Release
  OUTDIR := ../../..
  CPPFLAGS := -MD -D "HAVE_MMAP" -I "../../"
  CFLAGS += $(CPPFLAGS) -fPIC -O2
  CXXFLAGS := $(CFLAGS)
  LDFLAGS += -L$(BINDIR) -L$(LIBDIR) -shared -s -dynamiclib
  LDDEPS :=
  TARGET := libclaro-base.so
  BLDCMD = $(CC) -o $(OUTDIR)/$(TARGET) $(OBJECTS) $(LDFLAGS) $(RESOURCES)
endif

OBJECTS := \
	$(OBJDIR)/block.o \
	$(OBJDIR)/hashtable.o \
	$(OBJDIR)/claro.o \
	$(OBJDIR)/list.o \
	$(OBJDIR)/log.o \
	$(OBJDIR)/memory.o \
	$(OBJDIR)/object.o \
	$(OBJDIR)/oscompat.o \
	$(OBJDIR)/store.o \
	$(OBJDIR)/object_map.o \
	$(OBJDIR)/string.o \

CMD := $(subst \,\\,$(ComSpec)$(COMSPEC))
ifeq (,$(CMD))
  CMD_MKBINDIR := mkdir -p $(BINDIR)
  CMD_MKLIBDIR := mkdir -p $(LIBDIR)
  CMD_MKOUTDIR := mkdir -p $(OUTDIR)
  CMD_MKOBJDIR := mkdir -p $(OBJDIR)
else
  CMD_MKBINDIR := $(CMD) /c if not exist $(subst /,\\,$(BINDIR)) mkdir $(subst /,\\,$(BINDIR))
  CMD_MKLIBDIR := $(CMD) /c if not exist $(subst /,\\,$(LIBDIR)) mkdir $(subst /,\\,$(LIBDIR))
  CMD_MKOUTDIR := $(CMD) /c if not exist $(subst /,\\,$(OUTDIR)) mkdir $(subst /,\\,$(OUTDIR))
  CMD_MKOBJDIR := $(CMD) /c if not exist $(subst /,\\,$(OBJDIR)) mkdir $(subst /,\\,$(OBJDIR))
endif

.PHONY: clean

$(OUTDIR)/$(TARGET): $(OBJECTS) $(LDDEPS) $(RESOURCES)
	@echo Linking claro-base
	-@$(CMD_MKBINDIR)
	-@$(CMD_MKLIBDIR)
	-@$(CMD_MKOUTDIR)
	@$(BLDCMD)

clean:
	@echo Cleaning claro-base
	-@rm -rf $(OUTDIR)/$(TARGET) $(OBJDIR)

